{
  "type": "MANIFEST",
  "version": "1.0",
  "author": "Anton Gorodnyanskiy",
  "brand": "TACTIC",
  "name": "Simple Boilerplate",
  "created": "27/12/2019",
  "updated": "05/01/2021",
  "editor": {
    "url": "editor.html",
    "version": "1.1"
  },
  "sizes": [
    {
      "type": "FIXED",
      "name": "160x600",
      "width": 160,
      "height": 600,
      "index": "160x600.min.html",
      "fallback": {
        "static": "160x600.png"
      }
    },
    {
      "type": "FIXED",
      "name": "300x600",
      "width": 300,
      "height": 600,
      "index": "300x600.min.html",
      "fallback": {
        "static": "300x600.png"
      }
    },
    {
      "type": "FIXED",
      "name": "580x400",
      "width": 580,
      "height": 400,
      "index": "580x400.min.html",
      "fallback": {
        "static": "580x400.png"
      }
    },
    {
      "type": "FIXED",
      "name": "980x150",
      "width": 980,
      "height": 150,
      "index": "980x150.min.html",
      "fallback": {
        "static": "980x150.png"
      }
    },
    {
      "type": "FIXED",
      "name": "980x300",
      "width": 980,
      "height": 300,
      "index": "980x300.min.html",
      "fallback": {
        "static": "980x300.png"
      }
    }
  ],
  "data": {
    "editor": {
      "defaults": {
        "BACKGROUND_IMAGE": {
          "preset": {
            "params": {
              "scale": "fill",
              "resize": {
                "method": "holder",
                "extend": 1
              },
              "polite": false,
              "blur": 0,
              "quality": 80,
              "ratio": 1
            }
          },
          "ratios": {
            "size_160x600": {
              "label": "160x600",
              "value": "160x600"
            },
            "size_300x600": {
              "label": "300x600",
              "value": "300x600"
            },
            "size_580x400": {
              "label": "580x400",
              "value": "580x400"
            },
            "size_980x150": {
              "label": "980x150",
              "value": "980x150"
            },
            "size_980x300": {
              "label": "980x300",
              "value": "980x300"
            }
          }
        }
      }
    },
    "banner": {
      "type": "BannerLayer",
      "clicktag": {
        "override": false,
        "url": "https://www.tacticrealtime.com/"
      },
      "fonts": {
        "font_regular": {
          "name": "font_regular",
          "url": "./assets/fonts/sourcecodepro-regular",
          "selector": [
            "div"
          ],
          "types": [
            "eot",
            "woff",
            "woff2"
          ]
        },
        "font_italic": {
          "name": "font_italic",
          "url": "./assets/fonts/sourcecodepro-regular",
          "selector": [
            "i"
          ],
          "types": [
            "eot",
            "woff",
            "woff2"
          ]
        },
        "font_bold": {
          "name": "font_bold",
          "url": "./assets/fonts/sourcecodepro-bold",
          "selector": [
            "b"
          ],
          "types": [
            "eot",
            "woff",
            "woff2"
          ]
        },
        "font_bold_italic": {
          "name": "font_bold_italic",
          "url": "./assets/fonts/sourcecodepro-bold",
          "selector": [
            "b i"
          ],
          "types": [
            "eot",
            "woff",
            "woff2"
          ]
        }
      },
      "params": {
        "wait": {
          "fonts": [
            "font_regular", "font_italic", "font_bold", "font_bold_italic"
          ],
          "assets": []
        },
        "stop": {
          "after": 30000
        }
      },
      "macros": {
        "var_1": {
          "value": "${var_1}",
          "global": true
        },
        "var_2": {
          "value": "${var_2}",
          "global": true
        },
        "var_3": {
          "value": "${var_3}",
          "global": true
        }
      },
      "layers": {
        "CANVAS": {
          "type": "JointLayer",
          "props": {
            "tension": {
              "enabled": false,
              "options": [
                [
                  0,
                  "tall_extra"
                ],
                [
                  0.5,
                  "tall"
                ],
                [
                  0.7,
                  "equal"
                ],
                [
                  1.8,
                  "wide"
                ],
                [
                  4.4,
                  "wide_extra"
                ]
              ]
            },
            "scale": {
              "enabled": false,
              "area": 10000,
              "options": [
                [
                  0,
                  "tiny",
                  2
                ],
                [
                  32000,
                  "small",
                  1
                ],
                [
                  64000,
                  "medium",
                  0
                ],
                [
                  256000,
                  "large",
                  0
                ]
              ]
            }
          },
          "layers": {
            "MESSAGE": {
              "type": "JointLayer",
              "layers": {
                "MESSAGE_TEXT": {
                  "type": "JointLayer",
                  "layers": {
                    "MESSAGE_TEXT_TITLE": {
                      "type": "TextLayer",
                      "params": {
                        "holder": "MESSAGE_TEXT",
                        "wrapper": "MESSAGE",
                        "tags": [
                          "h1"
                        ]
                      },
                      "sources": [
                        {
                          "text": "Hello, World!"
                        }
                      ]
                    },
                    "MESSAGE_TEXT_DESCRIPTION": {
                      "type": "TextLayer",
                      "params": {
                        "holder": "MESSAGE_TEXT",
                        "wrapper": "MESSAGE",
                        "tags": [
                          "p"
                        ]
                      },
                      "sources": [
                        {
                          "text": "It is <em>dynamic creative</em> powered by TACTIC. Banner size is $[size] pixels."
                        }
                      ]
                    },
                    "MESSAGE_TEXT_BUTTON": {
                      "type": "TextLayer",
                      "params": {
                        "holder": "MESSAGE_TEXT",
                        "wrapper": "MESSAGE",
                        "tags": [
                          "button",
                          "b"
                        ]
                      },
                      "sources": [
                        {
                          "text": "Learn More"
                        }
                      ]
                    }
                  }
                }
              }
            },
            "BACKGROUND": {
              "type": "JointLayer",
              "layers": {
                "BACKGROUND_IMAGE": null
              }
            },
            "LOGO": {
              "type": "JointLayer",
              "layers": {
                "LOGO_IMAGE": {
                  "type": "ImageLayer",
                  "params": {
                    "scale": "fit",
                    "align": [
                      "right",
                      "bottom"
                    ]
                  },
                  "sources": [
                    {
                      "width": 486,
                      "height": 128,
                      "url": "./assets/logo.svg"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}