  "data": {
    "banner": {
      "type": "BannerLayer",
      "clicktag": {
        "override": false,
        "synchronise": false,
        "encode": true,
        "url": "https://www.tacticrealtime.com/"
      },
      "macros": {
        "var_1": {
          "value": "${var_1}",
          "global": true,
          "attr": false
        },
        "var_2": {
          "value": "${var_2}",
          "global": true,
          "attr": false
        },
        "var_3": {
          "value": "${var_3}",
          "global": true,
          "attr": false
        }
      },
      "fonts": {
        "font_A_400": {
          "family": "font_A",
          "fallback": "Arial",
          "weight": 400,
          "sources": [
            {
              "url": "./assets/fonts/sourcecodepro-regular"
            }
          ],
          "types": [
            "woff2"
          ],
          "selector": [
            ".font_A_400 s",
            ".font_A_300 b"
          ]
        },
        "font_A_600": {
          "family": "font_A",
          "fallback": "Arial",
          "weight": 600,
          "sources": [
            {
              "url": "./assets/fonts/sourcecodepro-bold"
            }
          ],
          "selector": [
            ".font_A_600 s",
            ".font_A_400 b"
          ],
          "types": [
            "woff2"
          ]
        }
      },
      "params": {
        "wait": {
          "fonts": [
            "font_A_400", "font_A_600"
          ]
        },
        "stop": {
          "after": 30000
        }
      },
      "layers": {
        "CV": {
          "type": "JointLayer",
          "props": {
            "tension": {
              "name": "CV_tens",
              "options": [
                [
                  0,
                  "t2"
                ],
                [
                  0.4,
                  "t1"
                ],
                [
                  0.6,
                  "t0"
                ],
                [
                  0.9,
                  "w0"
                ],
                [
                  1.5,
                  "w1"
                ],
                [
                  4.0,
                  "w2"
                ]
              ]
            },
            "scale": {
              "name": "CV_scale",
              "area": 22000,
              "options": [
                [
                  0,
                  "xs"
                ],
                [
                  32001,
                  "s"
                ],
                [
                  80001,
                  "m"
                ],
                [
                  160001,
                  "l",
                  0
                ],
                [
                  320001,
                  "xl",
                  0
                ]
              ]
            },
            "size": {
              "name": "CV_size"
            }
          },
          "layers": {
            "LG": {
              "type": "JointLayer",
              "layers": {
                "LG_OBJ": {
                  "type": "JointLayer",
                  "layers": {
                    "LG_IMG": {
                      "type": "JointLayer",
                      "layers": [
                        {
                          "type": "ImageLayer",
                          "params": {
                            "wrapper": "LG",
                            "holder": "LG_IMG",
                            "scale": "fit",
                            "align": [
                              "right",
                              "bottom"
                            ]
                          },
                          "sources": [
                            {
                              "width": 486,
                              "height": 128,
                              "url": "./assets/logo.svg"
                            }
                          ],
                          "anims": {
                            "LG_anim": {
                              "name": "LG_anim"
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              }
            },
            "BG": {
              "type": "JointLayer",
              "macros": {
                "BG_place": {
                  "value": "fill",
                  "global": true,
                  "attr": {
                    "name": true
                  }
                }
              },
              "layers": {
                "BG_ARE": {
                  "type": "JointLayer"
                },
                "BG_OBJ": {
                  "type": "JointLayer",
                  "layers": {
                    "BG_IMG": {
                      "type": "JointLayer",
                      "layers": [
                        {
                          "type": "ImageLayer",
                          "params": {
                            "area": "BG_ARE",
                            "wrapper": "BG",
                            "holder": "BG_IMG",
                            "scale": "$[BG_place]",
                            "polite": false,
                            "ratio": 2,
                            "align": [
                              "center",
                              "middle"
                            ],
                            "format": "webp",
                            "resize": {
                              "method": "holder",
                              "extend": 1
                            },
                            "quality": {
                              "level": 80,
                              "velocity": 0
                            },
                            "blur": 0,
                            "sharpen": 0,
                            "crop": [
                              0,
                              0,
                              0,
                              0
                            ],
                            "excepts": {}
                          },
                          "sources": [
                            {
                              "width": 4000,
                              "height": 1988,
                              "url": "https://img.trtm.io/library/1518/47168/1/6de2a92019b42a1f28cbde43cfec5dad/original.jpg"
                            }
                          ],
                          "anims": {
                            "BG_anim": {
                              "name": "BG_anim"
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              }
            },
            "FG": {
              "type": "JointLayer",
              "layers": {
                "FG_OVR": {
                  "type": "JointLayer"
                }
              }
            },
            "MS": {
              "type": "JointLayer",
              "layers": {
                "MS_TXT": {
                  "type": "JointLayer",
                  "layers": {
                    "MS_TXT_0": {
                      "type": "TextLayer",
                      "params": {
                        "holder": "MS_TXT",
                        "wrapper": "MS",
                        "tags": [
                          "h1"
                        ]
                      },
                      "attrs": {
                        "font": {
                          "name": "font_A_600"
                        }
                      },
                      "sources": [
                        {
                          "text": "Hello, World!"
                        }
                      ],
                      "anims": {
                        "MS_anim": {
                          "name": "MS_anim"
                        }
                      }
                    },
                    "MS_TXT_1": {
                      "type": "TextLayer",
                      "params": {
                        "holder": "MS_TXT",
                        "wrapper": "MS",
                        "tags": [
                          "p"
                        ]
                      },
                      "attrs": {
                        "font": {
                          "name": "font_A_400"
                        }
                      },
                      "sources": [
